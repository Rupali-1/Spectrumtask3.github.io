<html lang='en'>
  <head>
    <title>Portfolio</title>
    <meta charset='utf-8' />
    <meta
      name='viewport'
      content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no'
    />
    <!-- <link rel="shortcut icon" type="image/x-icon" href="image/favicon.ico"> -->
    <link
      rel='stylesheet'
      href='https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.0.2/css/bootstrap.min.css'
      integrity='sha512-usVBAd66/NpVNfBge19gws2j6JZinnca12rAe2l+d+QkLU9fiG02O1X8Q6hepIpr/EYKZvKx/I9WsnujJuOmBA=='
      crossorigin='anonymous'
      referrerpolicy='no-referrer'
    />
    <link
      rel='stylesheet'
      href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css'
    />
    <link
      rel='stylesheet'
      href='https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.css'
      integrity='sha512-1cK78a1o+ht2JcaW6g8OXYwqpev9+6GqOkz9xmBN9iUUhIndKtxwILGWYOSibOKjLsEdjyjZvYDq/cZwNeak0w=='
      crossorigin='anonymous'
      referrerpolicy='no-referrer'
    />
    <link rel='stylesheet' type='text/css' href='./css/portfolio.css' />
  </head>
  <body>
    <header class='header'>
      <a href='#' class='logo'>Portfolio</a>
      <!-- set close button to d-lg-none d-md-none -->
      <a class='close-btn d-block d-lg-none'><i
          class='fa fa-navicon'
          style='font-size: 32px; color: white; padding: 5px; cursor: pointer'
        ></i></a>
      <nav class='d-none d-lg-flex justify-content-end align-items-center'>
        <a href='#home' class='active'>home</a>
        <a href='#about'>about</a>
        <a href='#skills'>skills</a>
        <a href='#education'>education</a>
        <a href='#contact'>contact</a>
        <a href='/form'>form</a>
        <a href='/logout' class='btn btn-dark text-white'>logout</a>
      </nav>
    </header>
    <!-- togglenav start -->
    <div class='togglenav'>
      <ul>
        <li><a href='#home' class='active'>home</a></li>
        <li><a href='#about'>about</a></li>
        <li><a href='#skills'>skills</a></li>
        <li><a href='#education'>education</a></li>
        <li><a href='#contact'>contact</a></li>
        <li><a href='/form'>form</a></li>
        <li><a href='/logout'>logout</a></li>
      </ul>
    </div>
    <!-- togglenav end -->
    <!-- home-start -->
    <section class='home' id='home'>
      <div class='container'>
        <div class='row home-row'>
          <div
            class='col-lg-12 col-12 flex-div flex-lg-row flex-md-column-reverse flex-sm-column-reverse flex-column-reverse'
          >
            <div class='details text-white'>
              <h3>Hello I'm</h3>
              <h1 class='text-capitalize'>{{user.fname}} {{user.lname}}</h1>
              <h4 class='text-capitalize'>{{user.domain}}</h4>
              <ul>
                <li>
                  <i class='fa fa-envelope' aria-hidden='true'></i>
                  <p>{{user.email}}</p>
                </li>
                <li>
                  <i class='fa fa-phone' aria-hidden='true'></i>
                  <p>{{user.mobile}}</p>
                </li>
                <li>
                  <i class='fa fa-map-marker' aria-hidden='true'></i>
                  <p>{{user.address}}</p>
                </li>
              </ul>
            </div>
            <div class='mypicdiv'></div>
          </div>
        </div>
      </div>
    </section>
    <!-- home end -->
    <!-- about satrt -->
    <section class='about' id='about'>
      <div class='container'>
        <div class='row px-2 py-4 d-block d-md-none d-lg-none'>
          <div class='col-lg-12 col-md-12 col-12'>
            <h1 class='text-white text-uppercase'>About Me</h1>
          </div>
        </div>
        <div class='row px-2'>
          <div class='col-lg-6 col-md-6 col-12 pb-3' data-aos='fade-up'>
            <img
              src='./images/backand_frontand.jpg'
              class='img-fluid'
              alt='img'
            />
          </div>
          <div
            class='col-lg-6 col-md-6 col-12 text-white d-flex flex-column justify-content-center px-4 pt-2 about-right'
            data-aos='fade-up'
            data-aos-delay='50'
          >
            <h1
              class='text-white d-none d-md-block d-lg-block text-uppercase'
            >About Me</h1>
            <p>
              {{user.bio}}
            </p>
            <div class='social-link'>
              <a href='#' target='_blank'><i
                  class='fa fa-facebook'
                  aria-hidden='true'
                ></i>
              </a>
              <a href='#' target='_blank'><i
                  class='fa fa-twitter'
                  aria-hidden='true'
                ></i>
              </a>
              <a href='#' target='_blank'><i
                  class='fa fa-linkedin'
                  aria-hidden='true'
                ></i>
              </a>
              <a href='#' target='_blank'><i
                  class='fa fa-google-plus'
                  aria-hidden='true'
                ></i>
              </a>
            </div>
            <div class='cvlink'>
              <a href='./files/Rupali_Mishra_Resume.pdf' download=''>Download CV
                <i class='fa fa-download' aria-hidden='true'></i>
              </a>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- about end -->
    <!-- More Info -->
    <section class='more-info'>
      <div class='container'>
        <h2 data-aos='fade-up'>More Info</h2>
        <ul
          class='info list-unstyled shadow'
          data-aos='fade-up'
          data-aos-delay='50'
        >
          <li>First Name: <span>{{user.fname}}</span></li>
          <li>Last Name: <span>{{user.lname}}</span></li>
          <li>Date of Birth: <span>{{user.dob}}</span></li>
          <li>Age: <span>{{user.age}}</span></li>
          <li>Email: <span>{{user.email}}</span></li>
          <li>Mobile: <span>{{user.mobile}}</span></li>
          <li>Address: <span>{{user.address}}</span></li>
        </ul>
      </div>
    </section>
    <!-- More Info end -->
    <!-- skills start -->
    <section class='skills' id='skills'>
      <div class='container'>
        <div class='row'>
          <div class='col-lg-12 col-md-12 col-12'>
            <h2
              class='skill-header text-center text-white py-4'
              data-aos='fade-up'
            >
              My Expertise
            </h2>
          </div>
        </div>
        <div class='row'>
          <div
            class='col-lg-6 col-md-6 col-12 skill-container'
            data-aos='fade-right'
            data-aos-delay='50'
          >
            <h3 class='text-center py-4 tec-skill'>Technical Skills</h3>
            <div class='skill-wrap text-center text-white'>
              <h4>{{user.skills.programming}}</h4>
            </div>
          </div>
          <div
            class='col-lg-6 col-md-6 col-12 skill-container'
            data-aos='fade-left'
            data-aos-delay='50'
          >
            <div>
              <h3 class='text-center py-4 tec-skill'>Frameworks</h3>
              <div
                class='skill-wrap text-center text-white'
                style='border-bottom: 1px solid #ccc;'
              >
                <h4>{{user.skills.frameworks}}</h4>
              </div>
            </div>
            <div>
              <h3 class='text-center py-4 tec-skill'>Other Skills</h3>
              <div class='skill-wrap text-center text-white'>
                <h4>{{user.skills.other}}</h4>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- skills end -->
    <!-- education & experience start -->
    <section class='experience' id='education'>
      <div class='container'>
        <div class='row px-2'>
          <div class='col-md-7 col-12'>
            <h2 class='text-white edu-header'>Education</h2>
            <div class='single-edu-box px-4' data-aos='fade-up'>
              <h4>
                Master Degree:
                <i>{{user.qualification.master.branch}}</i>
              </h4>
              <ul>
                <li>{{user.qualification.master.year}}</li>
                <li>{{user.qualification.master.college}}</li>
              </ul>
            </div>
            <div
              class='single-edu-box px-4 py-4'
              data-aos='fade-up'
              data-aos-delay='50'
            >
              <h4>
                Bachelor's Degree:
                <i>{{user.qualification.grad.branch}}</i></h4>
              <ul>
                <li>{{user.qualification.grad.year}}</li>
                <li>{{user.qualification.grad.college}}</li>
              </ul>
            </div>
            <div
              class='single-edu-box px-4 py-4'
              data-aos='fade-up'
              data-aos-delay='50'
            >
              <h4>
                Intermediate:
                <i>{{user.qualification.inter.branch}}</i></h4>
              <ul>
                <li>{{user.qualification.inter.year}}</li>
                <li>{{user.qualification.inter.college}}</li>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- educationend -->
    <!-- project -->
    <section class='project' id='project'>
      <div class='container'>
        <h2>Projects</h2>
        <div class='row'>
          {{#if user.projects.project1.title}}
            <div
              class='col-md-4 col-12 py-3 single-project'
              data-aos='fade-right'
              data-aos-delay='50'
            >
              <div class='panel'>
                <img src='./images/1.jpg' class='img-fluid' alt='' />
                <div class='body'>
                  <h4 class='py-3'>{{user.projects.project1.title}}</h4>
                  <a
                    href='{{user.projects.project1.link}}'
                    class='btn'
                    target='_blank'
                  >View Project</a>
                </div>
              </div>
            </div>
          {{/if}}
          {{#if user.projects.project2.title}}
            <div
              class='col-md-4 col-12 py-3 single-project'
              data-aos='fade-up'
              data-aos-delay='50'
            >
              <div class='panel'>
                <img src='./images/2.jpg' class='img-fluid' alt='' />
                <div class='body'>
                  <h4 class='py-3'>{{user.projects.project2.title}}</h4>
                  <a
                    href='{{user.projects.project2.link}}'
                    class='btn'
                    target='_blank'
                  >View Project</a>
                </div>
              </div>
            </div>
          {{/if}}
          {{#if user.projects.project3.title}}
            <div
              class='col-md-4 col-12 py-3 single-project'
              data-aos='fade-left'
              data-aos-delay='50'
            >
              <div class='panel'>
                <img src='./images/3.jpg' class='img-fluid' alt='' />
                <div class='body'>
                  <h4 class='py-3'>{{user.projects.project3.title}}</h4>
                  <a
                    href='{{user.projects.project3.link}}'
                    class='btn'
                    target='_blank'
                  >View Project</a>
                </div>
              </div>
            </div>
          {{/if}}
        </div>
      </div>
    </section>
    <!-- project end -->
    <!-- contact start -->
    <section class='contact' id='contact'>
      <div class='container'>
        <div class='row'>
          <div class='col-lg-12 col-md-12 col-12'>
            <h2 class='text-white py-4'>Get in Touch</h2>
          </div>
        </div>
        <div class='row px-2'>
          <div
            class='col-lg-5 col-md-5 col-12 py-4'
            style='padding: 0'
            data-aos='fade-right'
          >
            <iframe
              src='https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3244.1563039260327!2d-118.24138121907596!3d34.05372740503817!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x80c2c645e78d488d%3A0x8143d574ca713dc5!2sL.A.%20Design%20Studio%20-%20Web%20Design%20Company!5e1!3m2!1sen!2sin!4v1573929881269!5m2!1sen!2sin'
              frameborder='0'
              style='border: 0; height: 100%; width: 100%'
              allowfullscreen=''
            ></iframe>
          </div>
          <div
            class='col-lg-7 col-md-7 col-12'
            data-aos='fade-left'
            data-aos-delay='50'
          >
            <form class='form' action='/contact' method='POST'>
              <h3 class='form-label ms-3'>Send us a message</h3>
              <div class='w100 d-flex flex-lg-row flex-md-row flex-column'>
                <div class='w-100 me-2 d-flex flex-column'>
                  <label for=''>Full Name *</label>
                  <input
                    type='text'
                    class='input'
                    name='name'
                    maxlength='55'
                    placeholder='e.g. John Doe'
                    autocomplete='off'
                    required
                  />
                </div>
                <div class='w-100 ms-md-2 mt-3 mt-md-0 d-flex flex-column'>
                  <label for=''>Mobile</label>
                  <input
                    type='text'
                    class='input'
                    name='mobile'
                    minlength='10'
                    maxlength='15'
                    placeholder='e.g. +91-123 456 6890'
                    autocomplete='off'
                  />
                </div>
              </div>
              <div class='w100 d-flex flex-column'>
                <label for=''>Email *</label>
                <input
                  type='email'
                  class='input'
                  name='email'
                  placeholder='e.g. example@abc.com'
                  autocomplete='off'
                  required
                />
              </div>
              <div class='w100 d-flex flex-column'>
                <label for=''>Subject *</label>
                <input
                  type='text'
                  class='input'
                  name='subject'
                  maxlength='150'
                  placeholder='e.g. Need a website.'
                  autocomplete='off'
                  required
                />
              </div>
              <div class='w100 d-flex flex-column'>
                <label for=''>Message</label>
                <textarea
                  name='message'
                  maxlength='250'
                  placeholder='Maximum 250 characters allowed.'
                ></textarea>
              </div>
              <p class='warning text-danger'></p>
              <p class='success text-success'></p>
              <div class='w100'>
                <button type='submit' class='send' name='button'>
                  Drop Message
                </button>
              </div>
            </form>
          </div>
        </div>
      </div>
    </section>
    <!-- contact end -->
    <footer>
      <p class='text-center'>Copyright 2021. All Rights Reserved.</p>
      <p class='text-center'>Designed by Rupali Mishra.</p>
    </footer>

    <!-- script goes here -->
    <script
      src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js'
      integrity='sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ=='
      crossorigin='anonymous'
      referrerpolicy='no-referrer'
    ></script>
    <script
      src='https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.0.2/js/bootstrap.min.js'
      integrity='sha512-a6ctI6w1kg3J4dSjknHj3aWLEbjitAXAjLDRUxo2wyYmDFRcz2RJuQr5M3Kt8O/TtUSp8n2rAyaXYy1sjoKmrQ=='
      crossorigin='anonymous'
      referrerpolicy='no-referrer'
    ></script>
    <script
      src='https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.js'
      integrity='sha512-A7AYk1fGKX6S2SsHywmPkrnzTZHrgiVT7GcQkLGDe2ev0aWb8zejytzS8wjo7PGEXKqJOrjQ4oORtnimIRZBtw=='
      crossorigin='anonymous'
      referrerpolicy='no-referrer'
    ></script>

    <script type='text/javascript' src='./js/daisy.js'></script>
    <script type='text/javascript' src='./js/portfolio.js'></script>
  </body>
</html>